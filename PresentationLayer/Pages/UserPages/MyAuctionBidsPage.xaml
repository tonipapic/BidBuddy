<UserControl x:Class="PresentationLayer.Pages.MyAuctionBidsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PresentationLayer.Pages"
             Loaded="UserControl_Loaded"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <Button Content="Otvori aukciju" VerticalAlignment="Bottom" HorizontalAlignment="Right"  Margin="0,0,15,5" x:Name="btnOpenAuction" Click="btnOpenAuction_Click"></Button>
        <DataGrid x:Name="dgUserBids"  Loaded="dgUserBids_Loaded" FontSize="20" Margin="0,0,0,37"/>
        
    </Grid>
</UserControl>
